{
  "expo": {
    "name": "ClosetMATE",
    "slug": "closetmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#FFDCEF",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFDCEF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          // "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFDCEF"
        }
      ],
      [
        "expo-font",
        {
          "fonts":[
            "./assets/fonts/antic-didone-v16-latin-regular.ttf",
            "./assets/fonts/limelight-v19-latin-regular.ttf",
            "./assets/fonts/philosopher-v20-cyrillic_latin-700.ttf",
            "./assets/fonts/philosopher-v20-cyrillic_latin-regular.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission":"Select and upload images.",
          "cameraPermission":"Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
